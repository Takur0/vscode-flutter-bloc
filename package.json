{
	"name": "flutter-bloc",
	"displayName": "flutter-bloc",
	"description": "BLoC Pattern for Flutter Extention, writing Files.",
	"publisher": "yt1997kt",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:file",
        "onCommand:extension.helloWorld",
        "onCommand:flutterBloc.addScreenFiles"
	],
	"repository": "https://github.com/Takur0/vscode-flutter-bloc",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "flutterBloc.addScreenFiles",
				"title": "Create New Screen Files"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"__metadata": {
		"id": "be4d0e5a-36fd-4ca8-a2e9-9fd93ddec6d1",
		"publisherDisplayName": "Takur0",
		"publisherId": "10c99fd7-d46f-4165-b259-c5d975efc28a"
	}
}
